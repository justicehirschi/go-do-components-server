-if=\"page == 'sign_in'\" class=\"gradient fill-height\">\n      <v-layout>\n        <v-flex>\n          <v-card flat class=\"mt-5\" color=\"rgba(0,0,0,0.2)\">\n            <v-card-title class=\"display-3 font-weight-light\">SIGN-IN</v-card-title>\n            <v-form>\n              <div class=\"pl-4 pr-4 mb-2\"><i>{{errors[0]}}</i>{{user_created}}</div>\n              <v-text-field autofocus @keydown.enter=\"signIn()\" label=\"Email\" required outline color=\"pink darken-1\" class=\"pl-4 pr-4\" v-model=\"sign_in_email\"></v-text-field>\n              <v-text-field type=\"password\" @keydown.enter=\"signIn()\" label=\"Password\" required outline color=\"pink darken-1\" class=\"pl-4 pr-4\" v-model=\"sign_in_password\"></v-text-field>\n              <v-layout class=\"justify-end\">\n              <v-btn @click=\"signIn()\" round large depressed class=\"pink darken-1\">submit\n              </v-btn>\n            </v-layout>\n            </v-form>\n          </v-card>\n          <v-layout class=\"ma-4\" column align-center justify-center>\n          <v-flex>\n            <span class=\"pl-4\">Don't Have an Account?</span>\n          </v-flex>\n          <v-flex>\n            <v-btn @click=\"page = 'register'\" round depressed large class=\"pink darken-1\">Register Now</v-btn>\n          </v-flex>\n          </v-layout>\n        </v-flex>\n      </v-layout>\n    </v-container>\n\n<!--Register Page-->\n    <v-container v-if=\"page == 'register'\" class=\"gradient fill-height\">\n      <v-layout>\n        <v-flex>\n          <v-card flat color=\"rgba(0,0,0,0.2)\">\n            <v-card-title class=\"display-3 font-weight-light\">REGISTER</v-card-title>\n            <v-stepper v-model=\"reg_step\" class=\"transparent\" flat>\n              <v-stepper-items>\n\n              <v-stepper-content step=\"1\" flat>\n                <v-card flat class=\"transparent\">\n                  <v-form>\n                    <v-text-field @keydown.enter=\"regStep2()\" label=\"First Name\" outline flat color=\"pink darken-1\" class=\"pl-4 pr-4\" v-model=\"register_first_name\"></v-text-field>\n                    <v-text-field @keydown.enter=\"regStep2()\" label=\"Last Name\" outline flat color=\"pink darken-1\" class=\"pl-4 pr-4\" v-model=\"register_last_name\"></v-text-field>\n                    <v-text-field @keydown.enter=\"regStep2()\" label=\"age\" outline flat color=\"pink darken-1\" class=\"pl-4 pr-4\" v-model=\"register_age\"></v-text-field>\n                    <v-text-field @keydown.enter=\"regStep2()\" label=\"Email\" outline color=\"pink darken-1\" class=\"pl-4 pr-4\" v-model=\"register_email\"></v-text-field>\n                  </v-form>\n                  <div class=\"pl-4 pr-4 mb-2\"><i>{{errors[0]}}</i></div>\n                </v-card>\n                <v-btn @click=\"regStep2()\" large class=\"right pink darken-1\" depressed round>Next</v-btn>\n              </v-stepper-content>\n\n              <v-stepper-content step=\"2\">\n                <v-card flat class=\"transparent\">\n                  <v-form>\n                    <span class=\"pl-2\">Which City Are You Looking For Events In?</span>\n                    <v-text-field @keydown.enter.prevent=\"regStep3()\" color=\"pink darken-1\" label=\"Location\" outline flat class=\"pa-4\" v-model=\"register_city\"></v-text-field>\n                  </v-form>\n                  <div class=\"pl-4 pr-4 mb-2\"><i>{{errors[0]}}</i></div>\n                <v-btn @click=\"regStep3()\" large class=\"right pink darken-1\" depressed round>Next</v-btn>\n                <v-btn @click=\"reg_step = 1\" large class=\"right pink darken-1\" depressed round>Prev</v-btn>\n                </v-card>\n              </v-stepper-content>\n\n              <v-stepper-content step=\"3\">\n                <v-card flat class=\"transparent\">\n                  <v-form>\n                    <v-text-field @keydown.enter=\"addUser()\" label=\"Username\" color=\"pink darken-1\" outline flat class=\"pl-4 pr-4\" v-model=\"register_user_name\" maxlength=\"14\"></v-text-field>\n                    <v-text-field @keydown.enter=\"addUser()\" type=\"password\" label=\"Password\" color=\"pink darken-1\" outline flat class=\"pl-4 pr-4\" v-model=\"register_password\"></v-text-field>\n                    <v-text-field @keydown.enter=\"addUser()\" type=\"password\" label=\"Confirm Password\" color=\"pink darken-1\" outline flat class=\"pl-4 pr-4\" v-model=\"register_confirm_password\"></v-text-field>\n                  </v-form>\n                  <div class=\"pl-4 pr-4 mb-2\"><i>{{errors[0]}}</i></div>\n                  <v-btn @click=\"addUser()\" large class=\"right pink darken-1\" depressed round>Register</v-btn>\n                  <v-btn @click=\"regStep2()\" large class=\"right pink darken-1\" depressed round>Prev</v-btn>\n                </v-card>\n              </v-stepper-content>\n\n            </v-stepper-items>\n            <v-stepper-header>\n            <v-stepper-step color=\"pink darken-1\" step=\"1\">Step 1</v-stepper-step>\n            <v-stepper-step color=\"pink darken-1\" step=\"2\">Step 2</v-stepper-step>\n            <v-stepper-step color=\"pink darken-1\" step=\"3\">Step 3</v-stepper-step>\n          </v-stepper-header>\n            </v-stepper>\n          </v-card>\n        </v-flex>\n      </v-layout>\n    </v-container>\n\n<!--Home Page-->\n    <!-- <v-container v-if=\"page == 'home'\" class=\"gradient\">\n      <v-toolbar class=\"transparent\" tabs flat>\n        <v-toolbar-side-icon @click=\"drawer = '!drawer'\">\n        </v-toolbar-side-icon>\n        <v-tabs color=\"transparent\" slider-color=\"pink darken-1\">\n          <v-spacer></v-spacer>\n        <v-tab @click=\"tab = 'event_tab'\">Events</v-tab>\n        <v-tab to=\"/about\">Map</v-tab>\n        <v-tab @click=\"tab = 'chat_tab'\">\n            <v-icon>chat_bubble</v-icon>\n        </v-tab>\n\n        </v-tabs>\n      </v-toolbar>\n\n      <v-navigation-drawer app v-model=\"drawer\" width=\"200\">\n     